{
  "manifest_version": 3,
  "name": "Don't forget to apply for this job",
  "version": "1.0",
  "description": "A reminder for applying for jobs",
  "permissions": ["storage", "notifications", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
